#pragma enofi

use std::math::{PI}

trait Shape {
	fn area() int
}

struct Rectangle {
	width: int
	height: int
}

impl Shape for Rectangle {
	@inline
	fn &area() int {
		ret .width * .height
	}
}

struct Circle {
	r: f32
}

impl Shape for Circle {
	@inline
	fn &area() int {
		ret PI * .r * .r
	}
}

fn main() {
	rect: Shape = Rectangle{90, 5}
	circ: Shape = Circle{90.5}
	outln(rect.area())
	outln(circ.area())
}
