//jule:enofi

fn quicksort(mut s: []int) {
	if s.len <= 1 {
		ret
	}
	mut let i = 0
	unsafe {
		for i < s.len-1 {
			let (mut x, mut y) = &s[i], &s[i+1]
			if *x > *y {
				*x, *y = *y, *x
			}
			i++
		}
	}
	quicksort(s[:i])
	quicksort(s[i+1:])
}

fn main() {
	mut let my_slice = []int{1, 9, -2, 25, -24, 4623, 0, -1, 0xFD2}
	outln(my_slice)
	quicksort(my_slice)
	outln(my_slice)
}
