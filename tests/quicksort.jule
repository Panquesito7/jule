#pragma enofi

fn quicksort(slice: []int) {
	if slice.len <= 1 {
		ret
	}
	let i = 0
	for i < slice.len-1 {
		let (curr, next) = &slice[i], &slice[i+1]
		if *curr > *next {
			*curr, *next = *next, *curr
		}
		i++
	}
	quicksort(slice[:i])
	quicksort(slice[i+1:])
}

fn main() {
	let my_slice = []int{1, 9, -2, 25, -24, 4623, 0, -1, 0xFD2}
	outln(my_slice)
	quicksort(my_slice)
	outln(my_slice)
}
