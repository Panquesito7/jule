#pragma enofi

partition(&slice []int, low, high int) int {
	pivot: = slice[high]
	i: = low - 1
	for j: = low, j < high, j++ {
		if (slice[j] <= pivot) {
			i++
			slice[i], slice[j] = slice[j], slice[i]
		}
	}
	slice[i + 1], slice[high] = slice[high], slice[i + 1]
	ret i + 1
}

quicksort(&slice []int, low, high int) {
	if (low < high) {
		p: = partition(slice, low, high)
		quicksort(slice, low, p - 1)
		quicksort(slice, p + 1, high)
	}
}

main() {
	my_slice: = []int{1, 9, -2, 25, -24, 4623, 0, -1, 0xFD2}
	outln(my_slice)
	quicksort(my_slice, 0, my_slice.len-1)
	outln(my_slice)
}
