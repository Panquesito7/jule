// Maximum valid Unicode code point.
pub const MAX_RUNE = '\U0010FFFF'

// Represents invalid code points.
pub const REPLACEMENT_CHAR = '\uFFFD'

// Maximum ASCII value.
pub const MAX_ASCII = '\u007F'

// Maximum Latin-1 value.
pub const MAX_LATIN1 = '\u00FF'
