use std::fs::{File}
use sys for std::sys

// File handler for stdin.
pub static mut STDIN: &File = File.new(sys::STDIN)
// File handler for stdout.
pub static mut STDOUT: &File = File.new(sys::STDOUT)
// File handler for stderr.
pub static mut STDERR: &File = File.new(sys::STDERR)
