cpp type char: byte
cpp type wchar_t: u16

// Type alias for char type.
//
// Supports casting for:
//  - byte / u8
//  - i8
pub type Char: cpp.char

// Type alias for wchar_t type.
//
// Supports casting for:
//  - u16
pub type Wchar: cpp.wchar_t
