use cpp "<sys/stat.h>"

cpp type _mode_t: uint
cpp type _off_t: uint

cpp struct stat{
	st_mode: cpp._mode_t
	st_size: cpp._off_t
}

// Wrapper for C's stat.
pub struct Stat {
	pub st_mode: uint
	pub st_size: uint
}
